
  <body>
    <%- include("header") -%> 

   <div class = "box" id = "heading">
    <h1 name = "title"><%= List %></h1>
   </div>
    
    
    <div class="box">
      
        <% for(var i=0; i<newItem.length; i++){ %>
           <div class="item">
             <input type="checkbox">
             <p><%= newItem[i] %></p>
           </div>
         <% } %>
      
        <form class = "item" action="/" method="POST">
          <input type="text" placeholder="Add New" name="newItem" autocomplete="off">
          <button type="submit" name="list" value = <%= List %> >+</button>
        </form>

        
        
       
   
       
    </div>
    
  </body>
</html>

